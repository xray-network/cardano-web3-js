{
  "name": "cardano-web3-js",
  "version": "2.2.0",
  "author": "XRAY/Network <hello@xray.app>",
  "description": "Cardano Web3 JavaScript SDK",
  "license": "MIT",
  "homepage": "https://cardano-web3-js.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xray-network/cardano-web3-js.git"
  },
  "bugs": {
    "url": "https://github.com/xray-network/cardano-web3-js/issues"
  },
  "keywords": [
    "Cardano",
    "JavaScript",
    "API"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx test/__dev.ts",
    "build": "tsup src/index.ts",
    "format": "prettier --write \"**/*.{js,ts,mts,cts,json}\"",
    "test": "vitest run",
    "pub": "yarn build && yarn publish",
    "docs:start": "cd docs && yarn start",
    "docs:serve": "cd docs && yarn serve",
    "docs": "cd docs && yarn install && yarn build"
  },
  "browser": {
    "@dcspark/cardano-multiplatform-lib-nodejs": "@dcspark/cardano-multiplatform-lib-browser",
    "@emurgo/cardano-message-signing-nodejs": "@emurgo/cardano-message-signing-browser",
    "uplc-node": "uplc-web"
  },
  "dependencies": {
    "@dcspark/cardano-multiplatform-lib-browser": "^6.2.0",
    "@dcspark/cardano-multiplatform-lib-nodejs": "^6.2.0",
    "@emurgo/cardano-message-signing-browser": "^1.1.0",
    "@emurgo/cardano-message-signing-nodejs": "^1.1.0",
    "@sinclair/typebox": "^0.32.31",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.0.1",
    "blake2b": "^2.1.4",
    "buffer": "^6.0.3",
    "cardano-koios-client": "^1.0.10",
    "cardano-kupo-client": "^1.0.1",
    "cardano-nftcdn-client": "^1.0.6",
    "cardano-ogmios-client": "^1.0.0",
    "cbor-x": "^1.6.0",
    "cborg": "^4.2.10",
    "crypto-browserify": "^3.12.0",
    "js-sha256": "^0.11.0",
    "uplc-node": "^0.0.4",
    "uplc-web": "^0.0.4"
  },
  "devDependencies": {
    "@types/blake2b": "^2.1.3",
    "@types/node": "^20.12.7",
    "esbuild": "^0.21.5",
    "prettier": "^3.2.5",
    "tsup": "8.1.0",
    "tsx": "^4.15.1",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  }
}
